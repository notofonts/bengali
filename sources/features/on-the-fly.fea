# nukt

feature nukt {

script bng2;
lookup BNG_dflt_nukt_composition {
    lookupflag 0;
    sub rra-beng by dda-beng nukta-beng;
    sub rha-beng by ddha-beng nukta-beng;
    sub yya-beng by ya-beng nukta-beng;
} BNG_dflt_nukt_composition ;


# DECOMPOSE onTheFly
# WARNING: Using nukt for decomposition because in some environments ccmp is applied before reordering, so using ccmp could have unpredictable results.

lookup BNG_dflt_nukt_onTheFlyDecomp {
    lookupflag 0;
    ignore sub ka-beng' halant-beng ssa-beng;
    ignore sub nga-beng halant-beng ka-beng halant-beng @BNG_BA';
    # Add half-form ka arm
    sub ka-beng halant-beng [na-beng la-beng]' by [na-beng la-beng] ka-beng.arm;
    sub ka-beng halant-beng @BNG_BA' by @BNG_BA ka-beng.arm;
    sub ka-beng halant-beng ka-beng' by ka-beng ka-beng.part ka-beng.arm1;
    sub ka-beng halant-beng ta-beng' by ta-beng ka-beng.arm2;

    ignore sub nga-beng halant-beng ka-beng halant-beng @BNG_RA';
    sub ka-beng halant-beng @BNG_RA' by @BNG_RA ka-beng.arm2;

    # Add blwf ka arm
    ignore sub [ssa-beng sa-beng] halant-beng ka-beng' raphala-beng;
    sub [la-beng ssa-beng sa-beng] halant-beng ka-beng' by ka-beng ka-beng.part;

    # Sub blwf k_ta
    sub na-beng halant-beng ka-beng' halant-beng ta-beng by ta_uMatra-beng.blws;

    # Transform jha in ba + jha.part
    sub jha-beng halant-beng [@BNG_BA @BNG_RA]' by [@BNG_BA @BNG_RA] jh-beng.side;
    sub [ja-beng nya-beng] halant-beng jha-beng' by jha-beng jha-beng.part;
    sub @BNG_BA halant-beng jha-beng' by baphala-beng.alt4 jha-beng.part aaMatra-beng.side2;

    # Transform tta in ddha + flag
    sub tta-beng' [iiMatra-beng halant-beng] by ddha-beng tta-beng.flag;
    sub [iMatra-beng halant-beng] tta-beng' by ddha-beng tta-beng.flag;

    # Transform ttha in tha + top
    sub ttha-beng' [halant-beng] by ttha-beng.notop ttha-beng.top;
    sub [iMatra-beng halant-beng] ttha-beng' by ttha-beng.notop ttha-beng.top;
} BNG_dflt_nukt_onTheFlyDecomp;

lookup BNG_dflt_nukt_onTheFlyComp {
    lookupflag 0;
    sub ba-beng' nukta-beng' by ra-beng;
    sub na-beng halant-beng ta_uMatra-beng.blws halant-beng' ta-beng' by ka-beng.arm2;
}BNG_dflt_nukt_onTheFlyComp;

### TESTING
lookup BNG_dflt_nukt_onTheFly_swapNukta {
    lookupflag 0;
    sub [nna-beng pa-beng sha-beng sa-beng] nukta-beng' lookup BNG_dflt_nukt_swapNuktaHalant halant-beng' lookup BNG_dflt_nukt_swapNuktaHalant ddha-beng;
    sub [la-beng na-beng] nukta-beng' lookup BNG_dflt_nukt_swapNuktaHalant halant-beng' lookup BNG_dflt_nukt_swapNuktaHalant [ddha-beng dda-beng];
} BNG_dflt_nukt_onTheFly_swapNukta;

language PAL;

} nukt;



